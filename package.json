{
  "name": "ai-api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npx concurrently -p none \"next dev\" \"yarn start:api\" \"yarn start:playground\"",
    "dev:api-service": "npx concurrently \"yarn start:api\" \"yarn start:playground\"",
    "build": "next build",
    "start:api": "cd api-service && PYTHONUNBUFFERED=1 exec poetry run python app.py",
    "start:playground": "cd api-service/playground && yarn dev",
    "install:api-service": "cd api-service && poetry install && cd playground && yarn",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev",
    "db:generate": "prisma generate",
    "db:pull": "prisma db pull",
    "supabase:generate": "supabase gen types typescript --project-id \"ntjfcwpzsxugrykskdgi\" --schema public > db/supabase.ts",
    "postinstall": "yarn db:generate"
  },
  "dependencies": {
    "@devbookhq/splitter": "^1.4.0",
    "@prisma/client": "^4.11.0",
    "@supabase/auth-helpers-nextjs": "^0.5.5",
    "@supabase/auth-helpers-react": "^0.3.1",
    "@supabase/supabase-js": "^2.10.0",
    "@tiptap/extension-bullet-list": "^2.0.0-beta.220",
    "@tiptap/extension-code-block": "^2.0.0-beta.220",
    "@tiptap/extension-list-item": "^2.0.0-beta.220",
    "@tiptap/extension-ordered-list": "^2.0.0-beta.220",
    "@tiptap/extension-placeholder": "^2.0.0-beta.220",
    "@tiptap/pm": "^2.0.0-beta.220",
    "@tiptap/react": "^2.0.0-beta.220",
    "@tiptap/starter-kit": "^2.0.0-beta.220",
    "clsx": "^1.2.1",
    "highlight.js": "^11.7.0",
    "human-id": "^4.0.0",
    "immer": "^9.0.19",
    "lucide-react": "^0.122.0",
    "nanoid": "^4.0.1",
    "next": "13.2.3",
    "next-superjson-plugin": "^0.5.6",
    "prosemirror-state": "^1.4.2",
    "prosemirror-view": "^1.30.2",
    "randomcolor": "^0.6.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-markdown": "^8.0.5",
    "react-textarea-autosize": "^8.4.0",
    "sonner": "^0.1.6",
    "superjson": "^1.12.2",
    "swr": "^2.0.4",
    "tippy.js": "^6.3.7",
    "usehooks-ts": "^2.9.1",
    "zustand": "^4.3.5"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.9",
    "@types/node": "^18.14.6",
    "@types/randomcolor": "^0.5.7",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "cssnano": "^5.1.15",
    "date-fns": "^2.29.3",
    "eslint": "^8.35.0",
    "eslint-config-next": "^13.2.3",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-tailwindcss": "^3.10.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "postcss": "^8.4.21",
    "postcss-import": "^15.1.0",
    "prisma": "^4.11.0",
    "supabase": "^1.41.6",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.5"
  }
