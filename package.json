{
  "name": "@e2b/monorepo",
  "main": "index.js",
  "private": true,
  "scripts": {
    "version": "changeset version && pnpm run postVersion --workspaces --if-present",
    "publish": "changeset publish && pnpm run postPublish --workspaces --if-present",
    "test": "pnpm run test --workspaces --if-present"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@changesets/cli": "^2.26.2",
    "@changesets/read": "^0.5.9"
  },
  "packageManager": "pnpm@8.7.4"
}
